{
  "name": "zHome",
  "version": "1.15",
  "slug": "zhome",
  "description": "A custom Flask-React app integration for Home Assistant.",
  "arch": ["amd64", "aarch64", "armhf", "armv7"],
  "startup": "application",
  "boot": "auto",
  "options": {},
  "schema": {},
  "ports": {
    "5000/tcp": 5000
  },
  "webui": "http://[HOST]:[PORT:5000]/dashboard",
  "panel_icon": "mdi:island-variant",
  "ingress": true,
  "ingress_port": 5000,
  "ingress_entry": "/dashboard",
  "panel_title": "zHome",
  "homeassistant_api": true,
  "hassio_api": true,
  "hassio_role": "default",
  "services": ["mqtt:want"],
  "map": ["config:rw", "ssl:rw"],
  "host_network": true,
  "environment": {
    "FLASK_ENV": "production"
  }
}
